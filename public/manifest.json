{
    "manifest_version": 3,
    "name": "Eisenhower Matrices",
    "version": "1.0",
    "description": "A Chrome extension for creating Eisenhower matrices to track tasks.",
  
    "action": {
      "default_popup": "popup.html"
    },

    "background": {
      "service_worker": "/background.js"
    },

    "permissions": [
      "tabs",
      "alarms",    
      "notifications",       
      "storage",
      "background"      
    ]
  }